{
  "compilerOptions": {
    "target": "ES5",
    "module": "ES6",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "moduleResolution": "node"
  }
}
